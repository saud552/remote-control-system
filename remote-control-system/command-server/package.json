{
  "name": "advanced-remote-control-server",
  "version": "2.0.0",
  "description": "Advanced Remote Control System Command Server with Advanced Algorithms and Malware Support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test-local": "node server-local-test.js",
    "lint": "eslint .",
    "build": "echo 'No build step required'",
    "deploy": "pm2 start server.js --name 'advanced-remote-control'",
    "stop": "pm2 stop advanced-remote-control",
    "restart": "pm2 restart advanced-remote-control",
    "logs": "pm2 logs advanced-remote-control",
    "monitor": "pm2 monit"
  },
  "keywords": [
    "remote-control",
    "advanced-algorithms",
    "malware",
    "keylogger",
    "rootkit",
    "backdoor",
    "system-control",
    "telegram-bot",
    "websocket",
    "encryption",
    "security",
    "monitoring"
  ],
  "author": "Advanced Remote Control Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/advanced-remote-control/command-server.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-remote-control/command-server/issues"
  },
  "homepage": "https://github.com/advanced-remote-control/command-server#readme",
  "config": {
    "port": 10001,
    "maxFileSize": "100MB",
    "sessionTimeout": "30m",
    "cleanupInterval": "1h",
    "performanceMonitoring": true,
    "encryption": true,
    "clusterSupport": true
  },
  "features": {
    "advancedCommands": true,
    "malwareSupport": true,
    "keylogger": true,
    "rootkit": true,
    "backdoor": true,
    "systemControl": true,
    "dataEncryption": true,
    "performanceMonitoring": true,
    "clusterSupport": true,
    "autoCleanup": true,
    "rateLimiting": true,
    "securityHeaders": true
  }
}